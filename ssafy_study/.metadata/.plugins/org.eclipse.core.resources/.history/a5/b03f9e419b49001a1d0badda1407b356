package study.hw;

import java.util.Scanner;

public class Solution_D3_4615_재미있는오셀로게임 {

	static int T, N, M;
	static int[][] board;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		T = sc.nextInt();
		for(int t=1;t<=T;t++){
			N = sc.nextInt();
			M = sc.nextInt();
			board = new int[N+1][N+1];
			board[N/2][N/2]=1;
			board[N/2][N/2+1]=2;
			board[N/2+1][N/2]=2;
			board[N/2+1][N/2+1]=1;
			
			for(int i=1;i<N+1;i++){
				for(int j=1;j<N+1;j++){
					System.out.print(board[i][j]+" ");
				}
				System.out.println();
			}
			
		}
	}

}
