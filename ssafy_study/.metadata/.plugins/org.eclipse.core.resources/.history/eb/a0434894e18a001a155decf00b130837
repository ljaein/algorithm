package study0404;

import java.util.Scanner;
import java.util.TreeSet;

public class Solution_모의_5658_보물상자비밀번호 {

	static int T, N, K;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		T=sc.nextInt();
		for(int t=1;t<=T;t++){
			N=sc.nextInt();
			K=sc.nextInt();
			String s = sc.next();
			TreeSet<String> set = new TreeSet<>();
			for(int i=0;i<N/4-1;i++){
				for(int j=0;j<N;j+=3){
					System.out.println(s.substring(j,j+3));
					set.add(s.substring(j,j+3));
				}
				s = s.substring(N-1,N) + s.substring(1,N-2);
			}
		}
	}

}
