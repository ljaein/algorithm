package study0204;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.Scanner;

public class Solution_모의_2383_점심식사시간 {

	static int T,N;
	static int[][] map;
	static Point[] A,B;
	static Point[] stairs=new Point[2];
	static ArrayList<Point> peo = new ArrayList<>();
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		T=sc.nextInt();
		for(int t=1;t<=T;t++){
			N=sc.nextInt();
			map=new int[N][N];
			int idx=0;
			for(int i=0;i<N;i++){
				for(int j=0;j<N;j++){
					map[i][j]=sc.nextInt();
					if(map[i][j]!=1 && map[i][j]!=0){
						stairs[idx++]=new Point(i,j);
					}else if(map[i][j]==0){
						peo.add(new Point(i,j));
					}
				}
			}
			for(int i=0;i<peo.size();i++){
				A = new Point[i];
				B = new Point[peo.size()-i];
				boolean[] check = new boolean[peo.size()];
				combi(0,0,i,check);
			}
			
			
			
			
		}
	}
	
	public static void combi(int start, int cnt, int r,boolean[] check){
		if(cnt==r){
			int idx = 0;
			for(int i=0;i<peo.size();i++){
				if(!check[i])
					B[idx++]=peo.get(i);
			}
			System.out.println(Arrays.toString(A));
			System.out.println(Arrays.toString(B));
			return;
		}
		for(int i=start;i<peo.size();i++){
			A[cnt]=peo.get(i);
			check[i]=true;
			combi(i+1,cnt+1,r,check);
			check[i]=false;
		}
	}
	
	public static void down(){
		
	}
	
	static class Point{
		int y,x;

		public Point(int y, int x) {
			this.y = y;
			this.x = x;
		}
		
	}

}
