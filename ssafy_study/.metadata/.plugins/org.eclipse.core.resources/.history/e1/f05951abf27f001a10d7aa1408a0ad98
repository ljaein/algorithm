package study0403;

import java.util.Arrays;
import java.util.Scanner;

public class Main_11657_타임머신 {

	static int N, M;
	static Edge[] T;
	static int[] dist;

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		M = sc.nextInt();
		T = new Edge[M];
		dist = new int[N];
		Arrays.fill(dist, Integer.MAX_VALUE);
		for (int i = 0; i < M; i++) {
			int a = sc.nextInt() - 1;
			int b = sc.nextInt() - 1;
			int c = sc.nextInt();
			T[i] = new Edge(a, b, c);
		}
		solve();
		for(int i=1;i<N;i++){
			if(dist[i]==Integer.MAX_VALUE)
				System.out.println(-1);
			else
				System.out.println(dist[i]);
		}
		
	}
	
	static void solve(){
		dist[0] = 0;
		for(int i=0;i<N-1;i++){
			for(int j=0;j<M;j++){
				Edge cur = T[j];
				if(dist[cur.a]!=Integer.MAX_VALUE && dist[cur.b]>dist[cur.a]+cur.c)
					dist[cur.b]=dist[cur.a]+cur.c;
			}
		}
		//음수사이클?
		for(int j=0;j<M;j++){
			Edge cur = T[j];
			if(dist[cur.a]!=Integer.MAX_VALUE && dist[cur.b]>dist[cur.a]+cur.c){
				System.out.println(-1);
				return;
			}
		}
	}

	static class Edge {
		int a, b, c;

		public Edge(int a, int b, int c) {
			super();
			this.a = a;
			this.b = b;
			this.c = c;
		}

	}

}
