import java.util.Scanner;

public class Algo1_대전_6반_이재인 {

	static int T, S, N;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		T=sc.nextInt();
		for(int t=1;t<=T;t++){
			S=sc.nextInt();
			N=sc.nextInt();
			int in = S * N;
			int four = in/400;
			int one = in%400/100;
			int ten = in%400%100/10;
			System.out.println("#"+t+" "+four+" "+one+" "+ten);
		}
	}

}
