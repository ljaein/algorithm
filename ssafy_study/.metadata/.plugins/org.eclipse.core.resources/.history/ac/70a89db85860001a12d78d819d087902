package study0301;

import java.util.Arrays;
import java.util.Scanner;

public class Solution_D4_5671_지구온난화 {

	static int T, N, M;
	static String[] word;
	static char[] use;
	static boolean[] alph;

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		T = sc.nextInt();
		for (int t = 1; t <= T; t++) {
			N = sc.nextInt();
			M = sc.nextInt();
			word = new String[N];
			alph = new boolean[26];
			for (int i = 0; i < N; i++) {
				word[i] = sc.next();
				char[] c = word[i].toCharArray();
				for (int j = 0; j < c.length; j++) {
					System.out.println(c[j]);
					alph[c[j] - 'A'] = true;
				}
			}
			int len = 0;
			for (int i = 0; i < 26; i++) {
				if (alph[i])
					len++;
			}
			use = new char[len];
			int idx = 0;
			for (int i = 0; i < 26; i++) {
				if (alph[i])
					use[idx++] = (char) (i + 'A');
			}
			System.out.println(Arrays.toString(use));
		}
	}

	static void combi(int start, int cnt) {

	}

}
