package medical;

import java.util.List;
import person.Patient;

public class CDC extends Organization { // 질병관리본부
	List<Hospital> hospitalList;
	List<Patient> patientList;

	public CDC() {
	}

	public CDC(String name, int employeeCount, List<Hospital> hospitalList, List<Patient> patientList) {
		super(name, employeeCount);
		this.hospitalList = hospitalList;
		this.patientList = patientList;
	}
	
	public void about(){
		System.out.println("Organization - CDC "+super.getName());
	}
	
	public void about(String more){
		about();
		System.out.println("We manage Hospital and Patients");
	}
}
