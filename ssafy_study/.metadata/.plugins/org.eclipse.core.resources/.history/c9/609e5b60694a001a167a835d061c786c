package study.hw;

import java.util.Scanner;

public class Main_11399_ATM {

	static int N, min, res;
	static int[] time, sel;
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		time = new int[N];
		min = Integer.MAX_VALUE;
		res = 0;
		for(int i=0;i<N;i++){
			time[i] = sc.nextInt();
		}
		Perm(0,0);
		System.out.println(res);
		
	}
	
	public static void Perm(int flag, int cnt){
		if(cnt==N){
			int sum = 0;
			int[] stime = new int[N];
			for(int i=0;i<N;i++){
				stime[i] = time[sel[i]];
			}
			for(int i=0;i<N;i++){
				for(int j=0;j<i+1;j++){
					sum += stime[j];
				}
			}
			res = Math.min(res, sum);
			return;
		}
		for(int i=0;i<N;i++){
			if((flag & (1<<i))==0){
				sel[cnt]=i;
				Perm(flag | (1<<i), cnt+1);
			}
		}
	}

}
