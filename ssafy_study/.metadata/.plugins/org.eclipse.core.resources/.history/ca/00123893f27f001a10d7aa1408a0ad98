package study0403;

import java.util.Arrays;
import java.util.Scanner;

public class Main_11657_타임머신 {

	static int N, M;
	static Edge[] T;
	static int[] dist;

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		M = sc.nextInt();
		T = new Edge[M];
		dist = new int[N];
		Arrays.fill(dist, Integer.MAX_VALUE);
		for (int i = 0; i < M; i++) {
			int a = sc.nextInt() - 1;
			int b = sc.nextInt() - 1;
			int c = sc.nextInt();
			T[i] = new Edge(a, b, c);
		}
		dist[0] = 0;

	}

	static class Edge {
		int a, b, c;

		public Edge(int a, int b, int c) {
			super();
			this.a = a;
			this.b = b;
			this.c = c;
		}

	}

}
