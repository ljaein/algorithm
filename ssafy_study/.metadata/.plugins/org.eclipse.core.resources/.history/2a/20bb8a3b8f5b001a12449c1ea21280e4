package study0204;

import java.util.Scanner;

public class Solution_D4_7208_지도칠하기 {

	static int T, N,cnt;
	static int[] C;
	static int[][] map;

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		T = sc.nextInt();
		for (int t = 1; t <= T; t++) {
			N = sc.nextInt();
			cnt=0;
			C = new int[4];
			for (int i = 0; i < 4; i++) {
				C[i] = sc.nextInt();
			}
			map = new int[4][4];
			for (int i = 0; i < 4; i++) {
				for (int j = 0; j < 4; j++) {
					map[i][j] = sc.nextInt();
				}
			}
		}
	}

	public static void isLink() {
		for (int i = 0; i < 4; i++) {
			boolean ch = false;
			for (int j = i + 1; j < 4; j++) {
				if (map[i][j] == 1) {
					if (C[i] == C[j]) {
						C[i] = C[i] + 1 == 5 ? 1 : C[i] + 1;
						ch = true;
					}
				}
			}
			if(ch)
				cnt++;
		}
	}
}
