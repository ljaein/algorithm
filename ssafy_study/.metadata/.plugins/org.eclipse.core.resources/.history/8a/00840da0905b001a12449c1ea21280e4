package study0204;

import java.util.Scanner;

public class Solution_D4_7208_지도칠하기 {

	static int T, N;
	static int[] C;
	static int[][] map;

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		T = sc.nextInt();
		for (int t = 1; t <= T; t++) {
			N = sc.nextInt();
			C = new int[N];
			for (int i = 0; i < N; i++) {
				C[i] = sc.nextInt();
			}
			map = new int[N][N];
			for (int i = 0; i < N; i++) {
				for (int j = 0; j < N; j++) {
					map[i][j] = sc.nextInt();
				}
			}
			System.out.println("#" + t + " " + isLink());
		}
	}

	public static int isLink() {
		int cnt = 0;
		for (int i = 0; i < N; i++) {
			boolean ch = false;
			for (int j = i + 1; j < N; j++) {
				if (map[i][j] == 1) {
					if (C[i] == C[j]) {
						C[i] = C[i] + 1 == 5 ? 1 : C[i] + 1;
						ch = true;
					}
				}
			}
			if (ch)
				cnt++;
		}
		return cnt;
	}
}
