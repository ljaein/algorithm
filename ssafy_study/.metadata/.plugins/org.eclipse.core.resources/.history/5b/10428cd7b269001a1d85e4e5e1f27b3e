package study0303;

import java.util.Scanner;

public class Main_2565_전깃줄 {

	static int N;
	static int[][] rope;
	static boolean[] isrope;

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		rope = new int[N][2];
		isrope = new boolean[N];
		for (int i = 0; i < N; i++) {
			rope[i][0] = sc.nextInt();
			rope[i][1] = sc.nextInt();
			isrope[rope[i][0]] = true;
		}
		int max = 0;
		int maxidx = 0;
		for (int i = 0; i < N; i++) {
			if (isrope[i]) {
				for (int j = 0; j < i; j++) {
					if (rope[i][1] < rope[j][1]) {
						maxidx = j;
					}

				}
			}
		}
	}

}
