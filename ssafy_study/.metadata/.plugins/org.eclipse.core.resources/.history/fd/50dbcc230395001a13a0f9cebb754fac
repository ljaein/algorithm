package study0502;

import java.util.Comparator;
import java.util.PriorityQueue;
import java.util.Scanner;

public class Main_2252_줄세우기 {

	static int N, M;
	static int s, e;

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		M = sc.nextInt();
		PriorityQueue<Point> pq = new PriorityQueue<>();
		for (int i = 0; i < M; i++) {
			s = sc.nextInt();
			e = sc.nextInt();
			Point p = new Point(i,s,0);
			Point p2 = new Point(i,e,1);
			pq.add(p);
			pq.add(p2);
		}
		while(!pq.isEmpty()){
			System.out.println(pq.poll());
		}

	}
	
	static class Point implements Comparable<Point>{
		int idx , n, s;

		public Point(int idx, int n, int s) {
			super();
			this.idx = idx;
			this.n = n;
			this.s = s;
		}

		@Override
		public int compareTo(Point o) {
			if(this.idx == o.idx){
				if(this.s<o.s)
					return -1;
				else
					return 1;
			}else
				return -1;
		}
	}

}
