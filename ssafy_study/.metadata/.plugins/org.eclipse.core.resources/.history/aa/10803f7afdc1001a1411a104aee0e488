package study0702;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.StringTokenizer;

public class Main_19235_모노미노도미노 {

	static int[][] green, blue;
	static int N, num;
	static int[] dy = { -1, 0, 1, 0 };
	static int[] dx = { 0, 1, 0, -1 };

	public static void main(String[] args) throws NumberFormatException, IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		StringTokenizer st = null;
		N = Integer.parseInt(br.readLine());
		green = new int[6][4];
		blue = new int[6][4];
		num = 1;
		for (int i = 0; i < N; i++) {
			st = new StringTokenizer(br.readLine());
			int t = Integer.parseInt(st.nextToken());
			int y = Integer.parseInt(st.nextToken());
			int x = Integer.parseInt(st.nextToken());
			down(t, x, 0, green);
			down(t, y, 1, blue);
			full(green);
			full(blue);
			push(green);
			push(blue);
			
			num++;
		}
		print();
	}

	static void print() {
		for (int i = 0; i < 6; i++) {
			for (int j = 0; j < 4; j++) {
				System.out.print(green[i][j] + " ");
			}
			System.out.println();
		}
		System.out.println();
		for (int i = 0; i < 6; i++) {
			for (int j = 0; j < 4; j++) {
				System.out.print(blue[i][j] + " ");
			}
			System.out.println();
		}
		System.out.println("--------------");
	}

	static void push(int[][] arr) {
		boolean[][] visit = new boolean[6][4];
		for (int i = 5; i >= 0; i--) {
			for (int j = 0; j < 4; j++) {
				if (arr[i][j] == 0 || visit[i][j])
					continue;
				visit[i][j]=true;
				boolean flag = false;
				for (int d = 0; d < 4; d++) {
					int ny = i + dy[d];
					int nx = j + dx[d];
					if(ny<0||nx<0||ny>=6||nx>=4||visit[ny][nx]||arr[ny][nx]==0)
						continue;
					visit[ny][nx]=true;
					if (arr[i][j] == arr[ny][nx]) {
						if (d == 1 || d == 3)
							down(2, j, 0, arr);
						else if (d == 2 || d == 0)
							down(3, j, 0, arr);
						flag = true;
						break;
					}
				}
				if (!flag)
					down(1, j, 0, arr);
			}
		}
	}

	static void light(int[][] arr) {
		for (int j = 0; j < 4; j++) {
			if (arr[0][j] != 0) {
				for (int j2 = 0; j2 < 4; j++) {
					arr[5][j2] = 0;
				}
				break;
			}
		}
		for (int j = 0; j < 4; j++) {
			if (arr[1][j] != 0) {
				for (int j2 = 0; j2 < 4; j++) {
					arr[4][j2] = 0;
				}
				break;
			}
		}

	}

	static void full(int[][] arr) {
		for (int i = 0; i < 6; i++) {
			boolean check = true;
			for (int j = 0; j < 4; j++) {
				if (arr[i][j] == 0) {
					check = false;
					break;
				}
			}
			if (check) {
				for (int j = 0; j < 4; j++)
					arr[i][j] = 0;
			}
		}
	}

	static void down(int t, int n, int color, int[][] arr) {
		// int[][] arr = color == 0 ? green : blue;
		if (t == 1) {
			for (int i = 0; i < 6; i++) {
				if (arr[i][n] == 0) {
					if (i == 5)
						arr[i][n] = num;
					continue;
				} else {
					arr[i - 1][n] = num;
					break;
				}
			}
		} else if ((t == 2 && color == 0) || (t == 3 && color == 1)) {
			for (int i = 0; i < 6; i++) {
				if (arr[i][n] == 0 && arr[i][n + 1] == 0) {
					if (i == 5) {
						arr[i][n] = num;
						arr[i][n + 1] = num;
					}
					continue;
				} else {
					arr[i - 1][n] = num;
					arr[i - 1][n + 1] = num;
					break;
				}
			}
		} else if ((t == 3 && color == 0) || (t == 2 && color == 1)) {
			for (int i = 0; i < 6; i++) {
				if (arr[i][n] == 0) {
					if (i == 5) {
						arr[i][n] = num;
						arr[i - 1][n] = num;
					}
					continue;
				} else {
					arr[i - 1][n] = num;
					arr[i - 2][n] = num;
					break;
				}
			}
		}
	}

}
