package study0304;

import java.util.Scanner;

public class Main_15649_Nê³¼M1 {

	static int N, M;
	static int[] sel;

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		M = sc.nextInt();
		sel = new int[M];
	}

	static void Perm(int flag, int cnt) {
		if (cnt == M) {
			return;
		}
		for (int i = 0; i < N; i++) {
			if ((flag & (1 << i)) != 1) {
				sel[cnt] = i + 1;
				Perm(flag | (1 << i), cnt + 1);
			}
		}
	}

}
