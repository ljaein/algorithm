package study0201;

import java.util.Arrays;
import java.util.Scanner;

public class Solution_모의_4012_요리사 {

	static int T, N;
	static int[][] food;
	static int[] A,B;
	static boolean[] chk;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
//		T = sc.nextInt();
		N = sc.nextInt();
		A = new int[N/2];
		B = new int[N/2];
//		
//		food = new int[N][N];
//		for(int i=0;i<N;i++){
//			for(int j=0;j<N;j++){
//				food[i][j] = sc.nextInt();
//			}
//		}
		Comb(0,0);
		
	
	}
	public static void Comb(int start, int cnt){
		if(cnt==N/2){
			chk = new boolean[N];
			for(int i=0;i<N/2;i++){
				chk[A[i]]=true;
			}
			int idx = 0;
			for(int i=0;i<N;i++){
				if(!chk[i])
					B[idx++]=i;
			}
			System.out.println(Arrays.toString(A));
			System.out.println(Arrays.toString(B));
			return;
		}
		for(int i=start;i<N;i++){
			A[cnt]=i;
			Comb(i+1,cnt+1);
		}
	}

}
