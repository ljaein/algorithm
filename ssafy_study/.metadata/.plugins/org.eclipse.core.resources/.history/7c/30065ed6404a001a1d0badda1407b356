package study.hw;

import java.util.Scanner;

public class Main_2567_»öÁ¾ÀÌ2 {

	static int N;
	static int[][] map;
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		map = new int[102][102];
		N = sc.nextInt();
		for(int i=0;i<N;i++){
			int x = sc.nextInt();
			int y = sc.nextInt();
			map[y-1][x-1] = 1;
		}

		for(int i=0;i<N+2;i++){
			for(int j=0;j<N+2;j++){
				System.out.print(map[i][j]);
			}
			System.out.println();
		}
	}

}
