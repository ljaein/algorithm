package study0204;

import java.util.Scanner;

public class Solution_모의_2112_보호필름 {

	static int T, D, W, K;
	static int[][] film;

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		T = sc.nextInt();
		for (int t = 1; t <= T; t++) {
			D = sc.nextInt();
			W = sc.nextInt();
			K = sc.nextInt();
			film = new int[D][W];
			for (int i = 0; i < D; i++) {
				for (int j = 0; j < W; j++) {
					film[i][j] = sc.nextInt();
				}
			}
			System.out.println(check(film));
			
			
		}
	}
	
	public static boolean check(int[][] f){
		for(int j=0;j<W;j++){
			boolean chk = false;
			int cnt = 0;
			f:for(int a=0;a<D-1;a++){
				for(int b=1;b<D;b++){
					if(f[a][j]==f[b][j]){
						cnt++;
						if(cnt==2){
							System.out.println(j);
							chk = true;
							break f;
						}
					}else
						cnt=0;
				}
			}
			if(!chk)
				return false;
		}
		return true;
	}

}
