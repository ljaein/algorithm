package study0203;

import java.util.Scanner;

public class Main_3109_빵집 {

	static int R, C, cnt, max;
	static char[][] map;
	static boolean[][] visited;
	static int[] dy = { -1, 0, 1 };
	static int[] dx = { 1, 1, 1 };

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		R = sc.nextInt();
		C = sc.nextInt();
		cnt = 0;
		map = new char[R][C];
		for (int i = 0; i < R; i++) {
			map[i] = sc.next().toCharArray();
		}

		visited = new boolean[R][C];
		visited[0][0] = true;
		dfs(0, 0);
		System.out.println(max);

	}

	public static void ddfs() {
		for (int r = 0; r < R; r++) {
			int y = r;
			int x = 0;
			for (int i = 0; i < 3; i++) {
				int ny = y + dy[i];
				int nx = x + dx[i];
				if (ny < 0 || nx < 0 || ny >= R || nx >= C || visited[ny][nx])
					continue;
				if (map[ny][nx] == '.') {
					visited[ny][nx] = true;
					if(nx==C-1){
						cnt++;
						if(r==R-1){
							max = Math.max(max, cnt);
							cnt=0;
						}
						return;
					}
					
					dfs(ny, nx);
					visited[ny][nx] = false;
				}
			}
		}
	}

	public static void dfs(int y, int x) {
		for (int r = 0; r < R; r++) {
			if (x == C - 1) {
				cnt++;
				return;
			}
			for (int i = 0; i < 3; i++) {
				int ny = y + dy[i];
				int nx = x + dx[i];
				if (ny < 0 || nx < 0 || ny >= R || nx >= C || visited[ny][nx])
					continue;
				if (map[ny][nx] == '.') {
					visited[ny][nx] = true;
					dfs(ny, nx);
					visited[ny][nx] = false;
				}
			}
		}
	}
}
