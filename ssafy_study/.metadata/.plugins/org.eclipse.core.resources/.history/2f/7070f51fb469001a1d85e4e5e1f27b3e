package study0303;

import java.util.Arrays;
import java.util.Scanner;

public class Main_2565_전깃줄 {

	static int N;
	static int[][] rope;
	static boolean[] isrope;

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		rope = new int[N][2];
		isrope = new boolean[N];
		for (int i = 0; i < N; i++) {
			rope[i][0] = sc.nextInt();
			rope[i][1] = sc.nextInt();
			isrope[rope[i][0]] = true;
		}
		System.out.println(Arrays.toString(isrope));
		for (int i = 0; i < N; i++) {
			if (isrope[i]) {
				boolean flag = false;
				for (int j = 0; j < i; j++) {
					if (isrope[j]) {
						if (rope[i][1] < rope[j][1]) {
							if (!flag) {
								isrope[j] = false;
								flag = true;
							} else {
								isrope[i] = false;
								break;
							}
						}
					}
				}
			}
		}
	}

}
