package study0304;

import java.util.Scanner;

public class Main_1600_말이되고픈원숭이 {

	static int K, W, H;
	static int[][] map;
	static int[] n_dy = { -1, -2, -2, -1, 1, 2, 2, 1 };
	static int[] n_dx = { -2, -1, 1, 2, 2, 1, -1, -2 };
	static int[] dy = { -1, 0, 1, 0 };
	static int[] dx = { 0, 1, 0, -1 };

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		K = sc.nextInt();
		W = sc.nextInt();
		H = sc.nextInt();
		map = new int[H][W];
		for (int i = 0; i < H; i++) {
			for (int j = 0; j < W; j++) {
				map[i][j] = sc.nextInt();
			}
		}

	}

	static void bfs() {

	}

	static class Point {
		int y, x, kcnt;

		public Point(int y, int x, int c) {
			super();
			this.y = y;
			this.x = x;
			this.kcnt = c;
		}

	}

}
