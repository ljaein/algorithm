package study0202;

import java.util.Scanner;

public class Main_17406_배열돌리기4 {

	static int N, M, K;
	static int[][] arr;
	static Rotate[] ro;
	static int[] sel;

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		M = sc.nextInt();
		K = sc.nextInt();
		arr = new int[N][M];
		for (int i = 0; i < N; i++) {
			for (int j = 0; j < M; j++) {
				arr[i][j] = sc.nextInt();
			}
		}
		ro = new Rotate[K];
		sel = new int[K];
		for (int i = 0; i < K; i++) {
			int r = sc.nextInt();
			int c = sc.nextInt();
			int s = sc.nextInt();
			ro[i] = new Rotate(r, c, s);
		}
		

	}
	public static void perm(int flag, int cnt){
		if(cnt==K){
			for(int i=0;i<K;i++){
				int r = ro[sel[i]].r;
				int c = ro[sel[i]].c;
				int s = ro[sel[i]].s;			
				rotation(r,c,s);
			}
			return;
		}
		for(int i=0;i<K;i++){
			if((flag & 1<<i)==0){
				sel[cnt]=i;
				perm(flag|1<<i,cnt+1);
			}
		}
	}
	
	public static void rotation(int[] sel){
		
	}

	static class Rotate {
		int r, c, s;

		public Rotate(int r, int c, int s) {
			this.r = r;
			this.c = c;
			this.s = s;
		}
	}

}
