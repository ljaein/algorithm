package study0303;

import java.util.Scanner;

public class Main_2437_저울 {

	static int N, cur, ans;
	static int[] arr, sel;

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		arr = new int[N];
		for (int i = 0; i < N; i++)
			arr[i] = sc.nextInt();
		//for (int i = 0; i < N; i++) {
			for (int j = 1; j < N; j++) {
				sel = new int[j];
				combi(0, 0, j, 3, 0);
			}
		//}
			System.out.println(ans);
	}

	static void combi(int start, int cnt, int r, int me, int num) {
		if (cnt == r) {
			int temp = cur;
			cur = num + arr[me];
			System.out.println(cur);
			if (cur != temp + 1 && cur != temp) {
				ans = temp;
			}
			return;
		}
		for (int i = start; i < r; i++) {
			num += arr[i];
			combi(i + 1, cnt + 1, r, me, num);
			num -= arr[i];
		}
	}

}
