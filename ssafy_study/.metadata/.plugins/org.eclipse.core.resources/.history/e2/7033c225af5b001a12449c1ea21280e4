package study0204;

import java.util.Arrays;
import java.util.Scanner;

public class Solution_D4_7208_지도칠하기 {

	static int T, N;
	static int[] C;
	static int[][] map;

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		T = sc.nextInt();
		for (int t = 1; t <= T; t++) {
			N = sc.nextInt();
			C = new int[N];
			for (int i = 0; i < N; i++) {
				C[i] = sc.nextInt();
			}
			map = new int[N][N];
			for (int i = 0; i < N; i++) {
				for (int j = 0; j < N; j++) {
					map[i][j] = sc.nextInt();
				}
			}
			isLink();
			System.out.println("#" + t + " " );
		}
	}

	public static void isLink() {
		for (int i = 0; i < N; i++) {
			System.out.println("i "+i);
			boolean[] selC = new boolean[4];
			for (int j = 0; j < N; j++) {
				if (map[i][j] == 1) {
					selC[C[j]-1]=true;
					System.out.println(C[j]-1);
				}
			}
			for(int j=0;j<4;j++){
				if(!selC[j])
					C[i]=j+1;
			}
		}
		System.out.println(Arrays.toString(C));
	}
}
