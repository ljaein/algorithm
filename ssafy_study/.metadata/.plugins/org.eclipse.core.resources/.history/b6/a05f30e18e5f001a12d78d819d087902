package study0201;

import java.util.LinkedList;
import java.util.List;
import java.util.Stack;

public class test {
	
	public static void main(String[] args) {
	 List<Mal> list = new LinkedList<>();
	 List<Mal>[] arr = new List[5];
	 for(int i=0;i<5;i++){
		 arr[i]=new LinkedList<>();
	 }
	 Mal m = new Mal(0,1);
	 list.add(m);
	 arr[0].add(m);
	 for(int i=0;i<list.size();i++){
		 Mal mm = list.get(i);
		 int idx = arr[0].indexOf(mm);
		 System.out.println(idx);
	 }
	 
	}
	
	static class Mal{
		int y, x;
		public Mal(int y, int x){
			this.y=y;
			this.x=x;
		}
	}
}
