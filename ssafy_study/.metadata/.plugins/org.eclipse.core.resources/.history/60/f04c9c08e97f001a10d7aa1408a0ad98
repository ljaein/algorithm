package study0403;

import java.util.Scanner;

public class Main_2660_회장뽑기 {

	static int N;
	static int[][] adj;

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		adj = new int[N][N];
		for (int i = 0; i < N; i++) {
			for (int j = 0; j < N; j++) {
				if (i == j)
					continue;
				adj[i][j] = Integer.MAX_VALUE;
			}
		}
		while (true) {
			int a = sc.nextInt();
			int b = sc.nextInt();
			if (a == -1 && b == -1)
				break;
			adj[a][b] = 1;
			adj[a][b] = 1;
		}
	}

}
