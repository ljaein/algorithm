package study0303;

import java.util.Arrays;
import java.util.Scanner;

public class Main_2565_전깃줄 {

	static int N, cnt;
	static Point[] rope;
	static int[] dp;

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		rope = new Point[N];
		dp = new int[N];
		for (int i = 0; i < N; i++) {
			rope[i] = new Point(sc.nextInt(), sc.nextInt());
		}
		Arrays.sort(rope);
		for (int i = 1; i < N; i++) {
			boolean in = false;
			boolean isi = false;
			int idx = -1;
			for (int j = 0; j < i; j++) {
				if (rope[j].b != 0) {
					if (rope[i].b < rope[j].b) {
						if (in) {
							rope[i].b = 0;
							isi = true;
							break;
						}
						idx = j;
						in = true;
						cnt++;
					}
				}
			}
			if (!isi) {
				if (rope[i].b < rope[idx].b)
					rope[idx].b = 0;
				else
					rope[i].b = 0;
			}
		}
		System.out.println(cnt);

	}

	static class Point implements Comparable<Point> {
		int a, b;

		public Point(int a, int b) {
			super();
			this.a = a;
			this.b = b;
		}

		@Override
		public int compareTo(Point arg0) {
			if (this.a > arg0.a)
				return 1;
			else
				return -1;
		}

		@Override
		public String toString() {
			return "Point [a=" + a + ", b=" + b + "]";
		}

	}

}
