public class Main_3678_카탄의개척자 {

	static int N;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int[] arr = new int[10001];
		for(int i=0;i<5;i++)
			arr[i]=i+1;
		arr[5]=2;
		arr[6]=3;
		int mul = 0;
		int start = 6*mul;
		
		int T = sc.nextInt();
		for(int t=1;t<=T;t++){
			N=sc.nextInt();
		}
	}

}
