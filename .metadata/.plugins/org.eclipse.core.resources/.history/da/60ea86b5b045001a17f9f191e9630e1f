package study.hw;

import java.util.Arrays;

public class perm {
static int[] sel;
static int N;
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		sel = new int[3];
		N = 3;
		perm(0,0);

	}
	public static void perm(int flag, int cnt){
		if(cnt==N){
			System.out.println(Arrays.toString(sel));
			return;
		}
		for(int i=0;i<N;i++){
			if((flag & 1<<i) == 0){
				sel[cnt] = i;
				perm(flag | 1<<i,cnt+1);
			}
		}
	}
}
